{
    "name": "Co-Op Search",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Figure out where I've already applied",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>", 
        "storage"
    ], 
    "content_scripts": [
        {
            "matches": ["https://www.linkedin.com/jobs/saved/"],
            "js": ["highlightAppliedJobs.js"]
        }
    ]
}